@model IEnumerable<SQIndustryThree.Models.BillApproval.BillAprrovalPoDetails>
<div class="row">
    <div class="col-md-12">
        <table class="table table-responsive table-striped" id="POUploadTable">
            <thead>
                <tr>
                    <th style="text-align:center">PONumber</th>
                    <th style="text-align:center">Supplier</th>
                    <th style="text-align:center">Article</th>
                    <th style="text-align:center">Color</th>
                    <th style="text-align:center">Size</th>
                    <th style="text-align:center">POQty</th>
                    <th style="text-align:center">PoValue</th>
                    <th style="text-align:center">Rate</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td style="text-align:center">@item.PONumber</td>
                        <td style="text-align:center">@item.SupllierName</td>
                        <td style="text-align:center">@item.ArticleName</td>
                        <td style="text-align:center">@item.ColorName</td>
                        <td style="text-align:center">@item.SizeName</td>
                        <td style="text-align:center">@item.POQty</td>
                        <td style="text-align:center">@item.PoValue</td>
                        <td style="text-align:center">@item.Rate</td>
                    </tr>
                 }
            </tbody>
        </table>
    </div>
</div>